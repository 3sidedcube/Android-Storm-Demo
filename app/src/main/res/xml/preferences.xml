<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
	<PreferenceCategory
		android:title="Language"
	>
		<Preference
			android:key="locale"
			android:title="Change your apps language"
			android:summary="Automatic"
			android:enabled="true"
		/>
	</PreferenceCategory>

	<PreferenceCategory
		android:title="Developer"
	>
		<Preference
			android:key="developer_mode"
			android:title="Enter developer mode"
			android:enabled="true"
		/>
	</PreferenceCategory>
</PreferenceScreen>
